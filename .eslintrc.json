{
     "env": {
          "browser": true,
          "es2021": true
     },
     "extends": [
          "plugin:react/recommended",
          "airbnb",
          "plugin:@typescript-eslint/recommended"
     ],
     "parserOptions": {
          "ecmaFeatures": {
               "jsx": true
          },
          "ecmaVersion": 12,
          "sourceType": "module"
     },
     "plugins": ["react", "@typescript-eslint"],
     "rules": {
          "react/no-unescaped-entities": "off",
          "@next/next/no-page-custom-font": "off"
     }
}
